component Test() {
    let loading = Signal::new(false);
    let items = vec![1, 2, 3];

    return <div>
        {loading.get() ? (
            <div>Loading...</div>
        ) : (
            <div>
                {items.map(|x| {
                    <span>{x}</span>
                })}
            </div>
        )}
    </div>;
}
