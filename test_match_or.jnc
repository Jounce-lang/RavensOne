// Test file for match arm OR patterns
// Should support: 3 | 4 | 5 => ...

fn classify_number(n: i32) -> String {
    match n {
        1 => "one",
        2 => "two",
        3 | 4 | 5 => "three to five",
        6 | 7 | 8 | 9 => "six to nine",
        _ => "other",
    }
}

fn main() {
    println!("1 is {}", classify_number(1));
    println!("3 is {}", classify_number(3));
    println!("4 is {}", classify_number(4));
    println!("7 is {}", classify_number(7));
    println!("10 is {}", classify_number(10));
}
