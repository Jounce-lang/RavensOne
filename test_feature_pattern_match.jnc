// Test: Pattern matching
enum Status {
    Active,
    Inactive,
    Pending(i32),
}

fn main() {
    let status = Status::Pending(42);

    // Match with literals
    let x = 5;
    match x {
        1 => { let result = "one"; }
        2 => { let result = "two"; }
        _ => { let result = "other"; }
    }

    // Match with enums
    match status {
        Status::Active => { let msg = "active"; }
        Status::Inactive => { let msg = "inactive"; }
        Status::Pending(n) => { let msg = "pending with " + n.to_string(); }
        _ => { let msg = "unknown"; }
    }

    return 0;
}
