struct CartItem {
    product_id: i32,
    quantity: i32,
}

struct CartState {
    items: Vec<CartItem>,
}

fn remove_from_cart(product_id: i32) {
    let current_state = CartState {
        items: vec![],
    };
    let mut items = vec![];
    let target_id = product_id;  // Workaround: use variable instead of comparing directly

    for item in current_state.items {
        let should_keep = item.product_id != target_id;
        if should_keep {
            items.push(item);
        }
    }
}
