struct CartItem {
    product_id: i32,
    quantity: i32,
}

struct CartState {
    items: Vec<CartItem>,
}

fn remove_from_cart(product_id: i32) {
    let current_state = CartState {
        items: vec![],
    };
    let mut items = vec![];

    for item in current_state.items {
        if item.product_id != product_id {
            items.push(item);
        }
    }
}
